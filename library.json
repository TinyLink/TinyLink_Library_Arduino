{
    "name": "TinyLink_Library_Arduino",
    "version": "0.1.0",
    "keywords": "aliyun, tinylink, arduino, iot",
    "description": "A TinyLink library for Arduino",
    "license": "MIT",
    "homepage": "https://github.com/TinyLink/TinyLink_Library_Arduino",
    "repository": {
        "type": "git",
        "url": "https://github.com/TinyLink/TinyLink_Library_Arduino.git"
    },
    "authors": {
        "name": "tinylink group",
        "email": "tinylink@emnets.org"
    },
    "export": {
        "include": "src",
        "exclude": [
            "document",
            "examples",
            "*.md"
        ]

    },
    "dependencies": [
        {
            "name": "Paho",
            "version": "^1.0.0"
        },
        {
            "name": "SD"
        },
        {
            "name": "WiFiEsp",
            "version": "^2.2.2"
        }
    ],
    "build": {
        "flags": [
            "-I src/TinyLink",
            "-I src/TL_FILE/ARDUINO_FILE",
            "-I src/TL_HTTP/ARDUINO_HTTP",
            "-I src/TL_MQTT/ARDUINO_MQTT"
        ]
    },
    "frameworks": "arduino",
    "platforms": [
        "atmelavr"
    ]
}
